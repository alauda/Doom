{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "Preserve",
    "moduleResolution": "Bundler",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext"
  },
  "mdx": {
    "checkMdx": true
  }
}
