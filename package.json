{
  "name": "@alauda/doom",
  "version": "0.4.2",
  "type": "module",
  "description": "Doctor Doom making docs.",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab-ce.alauda.cn/idp/Doom.git"
  },
  "packageManager": "yarn@4.5.0",
  "engines": {
    "node": ">=18.17.0 <20 || >=20.1.0"
  },
  "bin": "lib/cli/index.js",
  "exports": {
    "./package.json": "./package.json",
    "./config": "./lib/config.js",
    "./runtime": "./lib/runtime/index.js"
  },
  "files": [
    "assets",
    "global.d.ts",
    "lib",
    "styles",
    "!lib/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "tsc -b src",
    "clean": "tsc -b src --clean",
    "dev": "yarn swc-node --watch-path src/cli --watch-path src/plugins src/cli/index.ts",
    "doom": "yarn swc-node src/cli/index.ts",
    "fixture": "yarn doom fixture-docs",
    "lint": "run-p 'lint:*'",
    "lint:es": "eslint .",
    "lint:tsc": "tsc -b",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "yarn build",
    "swc-node": "node --enable-source-maps --import @swc-node/register/esm-register"
  },
  "dependencies": {
    "@openapi-contrib/openapi-schema-to-json-schema": "^5.1.0",
    "@rspress/core": "^1.38.0",
    "@rspress/plugin-preview": "^1.38.0",
    "@shikijs/transformers": "^1.24.3",
    "chokidar": "^4.0.3",
    "clsx": "^2.1.1",
    "commander": "^12.1.0",
    "es-toolkit": "^1.30.1",
    "get-port": "^7.1.0",
    "openapi-types": "^12.1.3",
    "react-markdown": "^8.0.7",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1",
    "shiki": "^1.24.3",
    "swagger2openapi": "^7.0.8",
    "tinyglobby": "^0.2.10",
    "type-fest": "^4.30.2",
    "yaml": "^2.6.1",
    "yoctocolors": "^2.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@swc-node/register": "^1.10.9",
    "@swc/core": "1.10.1",
    "@types/eslint__js": "^8.42.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^18.19.68",
    "@types/react-dom": "^18.3.5",
    "@types/swagger2openapi": "^7.0.4",
    "eslint": "^9.17.0",
    "lint-staged": "^15.2.11",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "prettier-plugin-pkg": "^0.18.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
