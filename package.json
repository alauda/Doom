{
  "name": "@alauda/doom",
  "version": "0.1.0",
  "type": "module",
  "description": "Doctor Doom making docs.",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab-ce.alauda.cn/idp/Doom.git"
  },
  "packageManager": "yarn@4.5.0",
  "engines": {
    "node": ">=18.12.0"
  },
  "bin": "lib/cli/index.js",
  "exports": {
    "./package.json": "./package.json",
    "./runtime": "./lib/runtime/index.js"
  },
  "files": [
    "assets",
    "global.d.ts",
    "lib",
    "styles",
    "!lib/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "tsc -b src",
    "clean": "tsc -b src --clean",
    "dev": "yarn swc-node --watch-path src/cli --watch-path src/plugins src/cli/index.ts",
    "doom": "yarn swc-node src/cli/index.ts",
    "lint": "run-p 'lint:*'",
    "lint:es": "eslint .",
    "lint:tsc": "tsc -b --noEmit",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "yarn build",
    "swc-node": "node --enable-source-maps --import @swc-node/register/esm-register"
  },
  "dependencies": {
    "@rspress/core": "^1.36.0",
    "@rspress/plugin-preview": "^1.36.0",
    "chokidar": "^4.0.1",
    "commander": "^12.1.0",
    "rehype-raw": "^6.1.1",
    "yaml": "^2.6.0",
    "yoctocolors": "^2.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@swc-node/register": "^1.10.9",
    "@swc/core": "1.9.2",
    "@types/eslint__js": "^8.42.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "~18.13.0",
    "eslint": "^9.14.0",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "prettier-plugin-pkg": "^0.18.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
