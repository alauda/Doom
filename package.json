{
  "name": "@alauda/doom",
  "version": "0.20.3",
  "type": "module",
  "description": "Doctor Doom making docs.",
  "repository": "https://gitlab-ce.alauda.cn/idp/Doom.git",
  "packageManager": "yarn@4.9.1",
  "engines": {
    "node": ">=18.17.0 <20 || >=20.1.0"
  },
  "bin": "lib/cli/index.js",
  "exports": {
    "./package.json": "./package.json",
    "./config": "./lib/config.js",
    "./runtime": "./lib/runtime/index.js",
    "./types": "./lib/types.js"
  },
  "files": [
    "assets",
    "global.d.ts",
    "lib",
    "pyodide",
    "styles",
    "!lib/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "tsc -b src",
    "clean": "tsc -b src --clean",
    "dev": "yarn swc-node --watch-path src/cli --watch-path src/plugins src/cli/index.ts",
    "doom": "yarn swc-node src/cli/index.ts",
    "export": "yarn doom export",
    "fixture": "yarn dev fixture-docs",
    "format": "prettier --write .",
    "lint": "run-p 'lint:*'",
    "lint:es": "eslint . --cache",
    "lint:tsc": "tsc -b",
    "prepare": "simple-git-hooks && patch-package && yarn-berry-deduplicate",
    "prepublishOnly": "yarn build",
    "serve": "yarn doom serve",
    "swc-node": "node --enable-source-maps --import @swc-node/register/esm-register",
    "translate": "yarn doom translate fixture-docs"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.4.1",
    "@openapi-contrib/openapi-schema-to-json-schema": "^5.1.0",
    "@playwright/browser-chromium": "^1.52.0",
    "@rsbuild/plugin-yaml": "^1.0.2",
    "@rspress/core": "1.43.11",
    "@shikijs/transformers": "^3.3.0",
    "chokidar": "^4.0.3",
    "cli-progress": "^3.12.0",
    "clsx": "^2.1.1",
    "commander": "^13.1.0",
    "ejs": "^3.1.10",
    "es-toolkit": "^1.35.0",
    "html-tag-names": "^2.1.0",
    "mermaid": "^11.6.0",
    "openai": "^5.0.0-beta.0",
    "openapi-types": "^12.1.3",
    "p-ratelimit": "^1.0.1",
    "pdf-lib": "^1.17.1",
    "pdf-merger-js": "^5.1.2",
    "picomatch": "^4.0.2",
    "playwright": "^1.52.0",
    "react-markdown": "^8.0.7",
    "rehype-raw": "^6.1.1",
    "remark-directive": "^2.0.1",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "shiki": "^3.3.0",
    "simple-git": "^3.27.0",
    "swagger2openapi": "^7.0.8",
    "tinyglobby": "^0.2.13",
    "type-fest": "^4.40.0",
    "x-fetch": "^0.2.6",
    "yaml": "^2.7.1",
    "yoctocolors": "^2.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@swc-node/register": "^1.10.10",
    "@swc/core": "1.11.21",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/cli-progress": "^3.11.6",
    "@types/ejs": "^3.1.5",
    "@types/node": "^18.19.86",
    "@types/picomatch": "^4.0.0",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@types/swagger2openapi": "^7.0.4",
    "@unts/patch-package": "^8.1.1",
    "eslint": "^9.25.1",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import-x": "^4.10.6",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "prettier-plugin-pkg": "^0.19.0",
    "simple-git-hooks": "^2.12.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "yarn-berry-deduplicate": "^6.1.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
