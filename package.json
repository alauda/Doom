{
  "name": "@alauda/doom",
  "version": "0.14.19",
  "type": "module",
  "description": "Doctor Doom making docs.",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab-ce.alauda.cn/idp/Doom.git"
  },
  "packageManager": "yarn@4.7.0",
  "engines": {
    "node": ">=18.17.0 <20 || >=20.1.0"
  },
  "bin": "lib/cli/index.js",
  "exports": {
    "./package.json": "./package.json",
    "./config": "./lib/config.js",
    "./runtime": "./lib/runtime/index.js",
    "./types": "./lib/types.js"
  },
  "files": [
    "assets",
    "global.d.ts",
    "lib",
    "pyodide",
    "styles",
    "!lib/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "tsc -b src",
    "clean": "tsc -b src --clean",
    "dev": "yarn swc-node --watch-path src/cli --watch-path src/plugins src/cli/index.ts",
    "doom": "yarn swc-node src/cli/index.ts",
    "export": "yarn doom export",
    "fixture": "yarn dev fixture-docs",
    "lint": "run-p 'lint:*'",
    "lint:es": "eslint .",
    "lint:tsc": "tsc -b",
    "prepare": "simple-git-hooks && patch-package",
    "prepublishOnly": "yarn build",
    "serve": "yarn doom serve",
    "swc-node": "node --enable-source-maps --import @swc-node/register/esm-register",
    "translate": "yarn doom translate fixture-docs"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.4.0",
    "@openapi-contrib/openapi-schema-to-json-schema": "^5.1.0",
    "@playwright/browser-chromium": "^1.51.0",
    "@rspress/core": "1.43.5",
    "@shikijs/transformers": "^3.2.1",
    "chokidar": "^4.0.3",
    "cli-progress": "^3.12.0",
    "clsx": "^2.1.1",
    "commander": "^13.1.0",
    "ejs": "^3.1.10",
    "es-toolkit": "^1.33.0",
    "fs-extra": "^11.3.0",
    "html-tag-names": "^2.1.0",
    "mermaid": "^11.5.0",
    "openai": "^5.0.0-beta.0",
    "openapi-types": "^12.1.3",
    "p-ratelimit": "^1.0.1",
    "pdf-lib": "^1.17.1",
    "pdf-merger-js": "^5.1.2",
    "picomatch": "^4.0.2",
    "playwright": "^1.51.0",
    "react-markdown": "^8.0.7",
    "rehype-raw": "^6.1.1",
    "remark-directive": "^2.0.1",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "shiki": "^3.2.1",
    "simple-git": "^3.27.0",
    "swagger2openapi": "^7.0.8",
    "tinyglobby": "^0.2.12",
    "type-fest": "^4.37.0",
    "yaml": "^2.7.0",
    "yoctocolors": "^2.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@rsbuild/plugin-yaml": "^1.0.2",
    "@swc-node/register": "^1.10.10",
    "@swc/core": "1.11.9",
    "@types/cli-progress": "^3.11.6",
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^18.19.80",
    "@types/picomatch": "^3.0.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/swagger2openapi": "^7.0.4",
    "@unts/patch-package": "^8.1.1",
    "eslint": "^9.22.0",
    "lint-staged": "^15.5.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "prettier-plugin-pkg": "^0.18.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
